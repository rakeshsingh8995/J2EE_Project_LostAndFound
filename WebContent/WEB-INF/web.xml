<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>LostandFound</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
  <servlet-name>MyConn</servlet-name>
  <url-pattern>/fs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MyConn</servlet-name>
    <servlet-class>org.btm.lostandfound.MyConnection</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>FirstServ</servlet-name>
  <url-pattern>/ss</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FirstServ</servlet-name>
    <servlet-class>org.btm.lostandfound.FirstSevlet</servlet-class>
  </servlet>
</web-app>